{{#if initialized}}
  {{#let (get service.actives name) as |active| }}
    {{#let (component 'elsewhere-target' name=this.name) as |elsewhereTarget| }}
      {{#let (if active.lastObject.component (component active.lastObject.component) elsewhereTarget ) as |c| }}
      {{#if hasBlock}}
        {{yield c active.lastObject.outsideParams}}
      {{else}}
        {{c}}
      {{/if}}
    {{/let}}
    {{/let}}
  {{/let}}
{{/if}}

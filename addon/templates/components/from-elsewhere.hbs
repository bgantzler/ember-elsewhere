{{#if initialized}}
  {{log "initialized target" this.name}}
  {{log "service.targets" this.service.targets}}
  {{#let (get this.service.activeInfo this.name) as |activeInfo| }}
    {{log "target activeInfo" this.name activeInfo}}
    {{#if activeInfo}}
        {{log "activeInfo.component" activeInfo.component}}
        {{#if hasBlock}}
          {{yield activeInfo.component activeInfo.outsideParams}}
        {{else}}
          {{component activeInfo.component}}
        {{/if}}
    {{/if}}
  {{/let}}
{{/if}}

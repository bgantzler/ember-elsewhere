{{#if initialized}}
  {{#let (get this.service.activeInfo this.name) as |activeInfo| }}
    {{#if activeInfo}}
      {{#let (if activeInfo.component activeInfo.component (component 'elsewhere-target' name=this.name multiple=this.multiple) ) as |c| }}
        {{#if hasBlock}}
          {{yield c activeInfo.outsideParams}}
        {{else}}
          {{component c}}
        {{/if}}
      {{/let}}
    {{/if}}
  {{/let}}
{{/if}}
